        <% for (var i=0; i < sections.length;  i++ ) { %>
          <a href="/<%- sections[i] %>"><%- sections[i] %></a>
        <% } %>
    </div>
  </nav>
<main style="max-width:1000px; margin: auto; padding:0 .67em">
  <div class="title-container"><h1 class="title-h1">Tech stories, news, how to guides</h1></div>
  <% for (var i=0; i < sections.length;  i++ ) { %>
    <div class="list-card">
      <h3><%- sections[i] %></h3>
      <% for (var a=0; a < articles.length;  a++ ) { %>
        <% if (articles[a].section == sections[i]) { %>
          <a href="/<%- articles[a].section %>/<%- articles[a].canonical %>"><%- articles[a].title %></a>
        <% } %>
      <% } %>
    </div>
  <% } %>
  
</main>
